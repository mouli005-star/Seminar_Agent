# SeminarAgent Environment Configuration
# Copy this file to .env and fill in your actual values

# Required: Your OpenAI API key
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Customize OpenAI model (default: gpt-4)
OPENAI_MODEL=gpt-4

# Optional: Set custom temperature for LLM responses (default: 0.1)
OPENAI_TEMPERATURE=0.1

# Optional: Set maximum execution time in seconds (default: 3600)
MAX_EXECUTION_TIME=3600

# Optional: Set logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Optional: Enable verbose output (true/false)
VERBOSE=false

